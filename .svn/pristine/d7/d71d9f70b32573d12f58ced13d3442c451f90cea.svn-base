        <script>
           $(document).ready(function (){
            
            $("#search-product").click(function (){
                var prodCode = $("#product-code").val();
                if(prodCode==="203") {
                    alert("Product Found!");
                } else {
                    alert("Product Not Found!");
                }
            });

            $("#tabs").tabs();

            var btnFinish = $("<button></button>").text("Finish")
                                .addClass("btn btn-info")
                                .on("click", function() {
                                    alert("Product Setup Will Save!");
                                });

            var btnCancel = $("<button></button>").text("Reset")
                                .addClass("btn btn-danger")
                                .on("click", function() {
                                    $("#smartwizard").smartWizard("reset");
                                });

            // Step show event
            $("#smartwizard").on("showStep", function(e, anchorObject, stepNumber, stepDirection, stepPosition) {
                $("#prev-btn").removeClass("disabled");
                $("#next-btn").removeClass("disabled");
                if(stepPosition==="first") {
                    $("#prev-btn").addClass("disabled");
                } else if(stepPosition==="last") {
                    $("#next-btn").removeClass("disabled");
                } else {
                    $("#prev-btn").removeClass("disabled");
                    $("#next-btn").removeClass("disabled");
                }
            });

            //SmartWizard Initialize
            $("#smartwizard").smartWizard({
                selected: 0,
                theme: 'dots',
                justified: true,
                darkMode: false,
                autoAdjustHeight: true,
                cycleSteps: false,
                backButtonSupport: true,
                enableURLhash: true,
                transition: {
                    animation: 'fade',
                    speed: '400',
                    easing: ''
                },
                toolbarSettings: {
                    toolbarPosition: 'bottom',
                    toolbarButtonPosition: 'right',
                    showNextButton: true,
                    showPreviousButton: true,
                    toolbarExtraButtons: [btnFinish, btnCancel]
                },
                anchorSettings: {
                    anchorClickable: true,
                    enableAllAnchors:true,
                    markDoneStep:true,
                    markAllPreviousStepsAsDone:true,
                    removeDoneStepOnNavigateBack:false,
                    enableAnchorOnDoneStep:true
                },
                keyboardSettings: {
                    keyNavigation: true,
                    keyLeft: [37],
                    keyRight: [39]
                },
                lang: {
                    next: 'Next',
                    previous: 'Previous'
                },
                diabledSteps: [],
                errorSteps:[],
                hiddenSteps:[]
            });

            var engTables=[];
            var chnTables=[];

            $("#prod-seq").tableDnD({
                onDragStart: function(table, row) {
                    console.log("Init Arrays: "+ engTables);
                    engTables=[];
                    chnTables=[];
                },
                onDrop: function(table, row) {
                    var rows = table.tBodies[0].rows;
                    var debugStr = "Row dropped was "+row.id + ". New order: ";

                    for (let i = 0; i < rows.length; i++) {
                        const element = rows[i];
                        
                        debugStr += rows[i].id+" ";

                        var engTableId = "#product-001-"+rows[i].id;
                        var chnTableId = "#product-002-"+ rows[i].id;

                        $(engTableId).hide();
                        $(chnTableId).hide();

                        engTables.push($(engTableId));
                        chnTables.push($(chnTableId));

                        $(engTableId).remove();
                        $(chnTableId).remove();
                    }

                    console.log(debugStr);

                    engTables.forEach(function(item, index) {
                        $("#prod-seq-eng").append(item);
                        console.log($("#"+item.attr("id")));
                        console.log($("#"+item.attr("id")).attr("style"));
                        $("#"+item.attr("id")).fadeIn(2000);
                    });

                    chnTables.forEach(function(item, index) {
                        $("#prod-seq-chn").append(item);
                        console.log($("#"+item.attr("id")));
                        console.log($("#"+item.attr("id")).attr("style"));
                        $("#"+item.attr("id")).fadeIn(2000);
                    });
                }
            });

           });
        </script>
